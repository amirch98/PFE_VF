@model IEnumerable<Activite>
@{
    ViewData["Title"] = "Index";
}


<table id="example" class="table table-bordered" style="width:100%;background-color: white;">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.De)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.À)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Allocation)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ActiviteID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.De)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.À)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contact.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Allocation.AllocationID)
                </td>
            </tr>
        }
    </tbody>
</table>
